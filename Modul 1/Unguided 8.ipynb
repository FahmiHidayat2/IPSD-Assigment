   "source": [
    "def reverse_words(input_string):\n",
    "    words = input_string.split()\n",
    "    reversed_words = [word[::-1] for word in words]\n",
    "    return reversed_words\n",
    "def main():\n",
    "    user_input = input(\"Masukkan string: \")\n",
    "    result = reverse_words(user_input)\n",
    "    print(result)\n",
    "main()\n"
   ]
